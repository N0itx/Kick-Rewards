{
  "manifest_version": 3,
  "name": "Kick Rewards",
  "version": "2.1",
  "description": "Genera una lista de los usuarios que canjearon recompensas",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "*://kick.com/*",
    "*://*.kick.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://dashboard.kick.com/*"],
      "js": ["js/jquery.min.js", "content.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "images/x16.png",
    "32": "images/x32.png",
    "64": "images/x64.png",
    "128": "images/x128.png"
  }
}